{
  "manifest_version": 3,
  "name": "PlaceholdURL",
  "version": "1.0.0",
  "description": "Fill placeholders in URLs and navigate with a single click",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup/index.html"
  }
}
